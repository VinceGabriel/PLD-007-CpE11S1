#include <iostream>
#include <cmath> 
using namespace std;

double hypotenuse(double a, double b) 
{
    return sqrt(a * a + b * b);
}

int main() 
{
    double a, b;
    cout << "Enter the two sides of the triangle: ";
    cin >> a >> b;

    cout << "Hypotenuse: " << hypotenuse(a, b) << endl;

    return 0;
    
}
