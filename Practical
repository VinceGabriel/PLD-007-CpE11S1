#include <iostream>
using namespace std;

int main ()
{
	
	int sum = 0;
	int count = 0;
	
	cout<< "Enter 10 numbers (less than 5):\n";
	
	int i;
	for (i = 0; i < 10; i++); {
	int num;
	cin>> num;
	
	if  (num < 5);
		sum += num;
	
	else if (num > 5)
	
	cout<< "Enter a number less than 5:\n";
	
		int num;
		int count = 0;
	
	else if (num = 5);
	sum += num;
	
	else;
	cout<< "Enter 10 numbers less than 5:\n";
	
	return 0;
}
