#include <iostream>
using namespace std;

int main() {
    int values[10] = {19, 3, 15, 7, 11, 9, 13, 5, 17, 1};

    std::cout << "Element     Value     Histogram\n";
    std::cout << "----------------------------------------\n";

    for (int i = 0; i < 10; ++i) {
        
        std::cout << "     " << i << "         " << values[i] << "     ";
        
        for (int j = 0; j < values[i]; ++j) {
            std::cout << '*';
        }
        std::cout << '\n'; 
    }

    return 0;
}
